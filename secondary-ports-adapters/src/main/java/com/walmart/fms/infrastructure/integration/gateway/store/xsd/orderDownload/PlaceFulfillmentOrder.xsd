                                                                                <xsd:element name="orderPriority"
                                                                                             type="pfx:Priority"
                                                                                             minOccurs="0"
                                                                                             maxOccurs="1"/>
                                                                                <!--Added manually on 2016/03/22-->
                                                                                <xsd:element name="orderLines"
                                                                                             maxOccurs="unbounded">
